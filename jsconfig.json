{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/components/*": ["components/*"],
      "@app/redux/*": ["lib/redux/*"],
      "@app/lambda/*": ["lib/lambda/*"],
      "@app/styles/*": ["styles/*"],
      "@app/pages/*": ["pages/*"],
      "@app/*": ["./*"]
    }
  },
  "exclude": [
    ".netlify",
    ".next",
    "lib/lambda",
    "lambda-build",
    "node_modules",
    "out"
  ]
}
